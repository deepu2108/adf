{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "sandeep-uat"
		},
		"blb_tbsource_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'blb_tbsource'"
		},
		"AzureSqlDatabase1_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'AzureSqlDatabase1'"
		},
		"azprivateendpoint_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'azprivateendpoint'"
		},
		"mgprivateendpoint_properties_privateLinkResourceId": {
			"type": "string",
			"defaultValue": "/subscriptions/f2b75c05-3ec5-46f7-a3f8-719f047d2c8c/resourceGroups/covid-reporting-rg/providers/Microsoft.Sql/servers/ittechbroserver"
		},
		"mgprivateendpoint_properties_groupId": {
			"type": "string",
			"defaultValue": "sqlServer"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "sandeep-uat_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"blb_tbsource_connectionString": {
						"value": "[parameters('blb_tbsource_connectionString')]"
					},
					"AzureSqlDatabase1_connectionString": {
						"value": "[parameters('AzureSqlDatabase1_connectionString')]"
					},
					"azprivateendpoint_connectionString": {
						"value": "[parameters('azprivateendpoint_connectionString')]"
					},
					"mgprivateendpoint_properties_privateLinkResourceId": {
						"value": "[parameters('mgprivateendpoint_properties_privateLinkResourceId')]"
					},
					"mgprivateendpoint_properties_groupId": {
						"value": "[parameters('mgprivateendpoint_properties_groupId')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}